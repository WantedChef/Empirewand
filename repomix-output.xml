            this.name = "Mephidic Reap";
            this.description = "Throws a boomerang scythe that damages and slows enemies.";
            this.cooldown = java.time.Duration.ofSeconds(15);
            this.spellType = SpellType.POISON;
        }

        @Override
        @NotNull
        public Spell<Void> build() {
