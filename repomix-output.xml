            }
            
            if (ticks >= durationTicks || meteorsSpawned >= meteorCount) {
                this.cancel();
                createFinalExplosion();
                return;
            }

            // Spawn meteors at random locations around the center
            if (ticks % 5 == 0 && meteorsSpawned < meteorCount) {
